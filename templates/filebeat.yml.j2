filebeat:
 prospectors:
  - input_type: log
    paths:
     - "/var/ossec/logs/alerts/alerts.json"
    document_type: json
    json.message_key: log
    json.keys_under_root: true
    json.overwrite_keys: true

output:
 logstash:
   hosts:
{% for host in wazuh_server_elasticsearch_servers %}
    - "{{ host }}"
{% endfor %}

